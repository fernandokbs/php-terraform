    "keywords": ["php", "terraform"],
    "homepage": "https://github.com/fernandokbs/php-terraform",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Fernando Garcia",
        "email": "fernando93d@gmail.com"
      }
    ],
    "require": {
        "symfony/process": "^5.0"
    },
    
    "require-dev": {
